  "name": "skrap",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --proxy-config=proxy-config.json",
    "start:stage": "ng serve --proxy-config=proxy-config-new.json",
    "start:ngrok": "ng serve --proxy-config=proxy-config.json",
    "start-ip": "ng serve --proxy-config=proxy-config.json --configuration=ip",
    "start-prod": "ng serve --aot=true --proxy-config=proxy-config.json",
    "start-docker": "ng serve --aot=true --proxy-config=proxy-config.docker.json --host=0.0.0.0 --source-map=false",
    "start-local": "ng serve --aot=true --proxy-config=proxy-config.local.json",
    "build": "ng build",
    "build-prod": "ng build --prod --aot=true --build-optimizer=true",
    "build-docker": "ng build --configuration=docker --aot=true --build-optimizer=true",
    "build-stage": "ng build --configuration=staging --aot=true --build-optimizer=true",
    "build-ip": "ng build --configuration=ip --aot=true --build-optimizer=true",
